{
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/NC-Freelance-Project/Exhibition-Curator/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/NC-Freelance-Project/Exhibition-Curator/(.*)",
      "dest": "/index.html"
    }
  ]
}
